<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mr.mapper.CarMapper">


    <select id="getCarListBySkuIdAndUserId" resultType="list" parameterType="int">
        select * from t_mall_shoppingcar
        where
            yh_id = #{userId}
    </select>


    <select id="getCarBySkuIdAndUserId" parameterType="int" resultType="com.mr.model.TMallShoppingcar">
        select * from t_mall_shoppingcar
        where
            sku_id = #{skuId}
        and
            yh_id = #{userId}
    </select>
</mapper>
